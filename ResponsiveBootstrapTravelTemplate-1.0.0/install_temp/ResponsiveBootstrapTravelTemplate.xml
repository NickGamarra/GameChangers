<macromedia-extension
  name="Responsive Bootstrap Travel Template"
  version="1.0.0"
  type="suite"
 id="ResponsiveBootstrapTravelTemplate"
 requires-restart="True">

  <products>
    <product name="Dreamweaver" minversion="12" version="12" primary="true" />
  </products>

  <author name="DMXzone.com" />

  <description>
    <![CDATA[
    Responsive Bootstrap Travel Template - another great DMXzone Dreamweaver template.
    For more info see http://www.dmxzone.com/go?21847
    ]]>
  </description>
  <update method="directlink" url="http://www.dmxzone.com/dmxExtensionDetail.asp?NewsId=21847" />

  <ui-access>
    <![CDATA[You can access it from the Insert / DMXzone Templates menu.]]>
  </ui-access>

  <files>

		<!-- Template Files Begin -->

    <file source="destination.html" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate" />
    <file source="index.html" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate" />
    <file source="reqExtensions.xml" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate" />
    <file source="search.html" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate" />
    <file source="bootstrap/css/bootstrap-responsive.css" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/css" />
    <file source="bootstrap/css/bootstrap-responsive.min.css" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/css" />
    <file source="bootstrap/css/bootstrap.css" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/css" />
    <file source="bootstrap/css/bootstrap.min.css" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/css" />
    <file source="bootstrap/ico/apple-touch-icon-114-precomposed.png" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/ico" />
    <file source="bootstrap/ico/apple-touch-icon-144-precomposed.png" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/ico" />
    <file source="bootstrap/ico/apple-touch-icon-57-precomposed.png" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/ico" />
    <file source="bootstrap/ico/apple-touch-icon-72-precomposed.png" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/ico" />
    <file source="bootstrap/ico/favicon.png" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/ico" />
    <file source="bootstrap/img/glyphicons-halflings-white.png" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/img" />
    <file source="bootstrap/img/glyphicons-halflings.png" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/img" />
    <file source="bootstrap/img/examples/browser-icon-chrome.png" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/img/examples" />
    <file source="bootstrap/img/examples/browser-icon-firefox.png" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/img/examples" />
    <file source="bootstrap/img/examples/browser-icon-safari.png" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/img/examples" />
    <file source="bootstrap/img/examples/slide-01.jpg" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/img/examples" />
    <file source="bootstrap/img/examples/slide-02.jpg" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/img/examples" />
    <file source="bootstrap/img/examples/slide-03.jpg" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/img/examples" />
    <file source="bootstrap/js/bootstrap.js" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/js" />
    <file source="bootstrap/js/bootstrap.min.js" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/js" />
    <file source="bootstrap/js/html5shiv.js" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/js" />
    <file source="bootstrap/js/jquery.js" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/js" />
    <file source="bootstrap/js/holder/holder.js" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/bootstrap/js/holder" />
    <file source="css/style.css" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/css" />
    <file source="Styles/dmxNotify.css" destination="$dreamweaver/configuration/Shared/DMXzone/Templates/ResponsiveBootstrapTravelTemplate/Styles" />

    <!-- Template Files End -->

		<!-- DMX Template Manager -->
		<file source="Installer/shared/dmxTemplateManager.htm"  destination="$dreamweaver/configuration/Commands" shared="true" />
    
		<!-- DMX FileUpdater -->
		<file source="Installer/shared/DMX/Components/UI/FileUpdater/dmxUpdateFiles.htm"           destination="$dreamweaver/configuration/Commands" shared="true" />
		<file source="Installer/shared/DMX/Components/UI/FileUpdater/dmxUpdateFilesDialog.htm"     destination="$dreamweaver/configuration/Commands" shared="true" />

		<!-- DMX HeadUpdater -->
		<file source="Installer/shared/DMX/Components/UI/HeadUpdater/dmxUpdateHead.htm"            destination="$dreamweaver/configuration/Commands" shared="true" />

		<!-- DMX ProgressIndicator -->
		<file source="Installer/shared/DMX/Components/UI/ProgressIndicator/ProgressIndicator.htm"   destination="$dreamweaver/configuration/Commands" shared="true" />
		<file source="Installer/shared/DMX/Components/UI/ProgressIndicator/img/progressbg.png"      destination="$dreamweaver/configuration/Shared/DMXzone/DMX/Components/UI/ProgressIndicator/img" shared="true" />
		<file source="Installer/shared/DMX/Components/UI/ProgressIndicator/img/progressvaluebg.png" destination="$dreamweaver/configuration/Shared/DMXzone/DMX/Components/UI/ProgressIndicator/img" shared="true" />
    
    <file source="install_temp/ResponsiveBootstrapTravelTemplate.xml" destination="$dreamweaver/configuration/Shared/DMXzone/InstalledExtensions" />
</files>
  
  <configuration-changes>
		<menu-insert insertAfter="DWMenu_Insert_Media">
			<menu name="DMXzone Templates" id="DWMenu_Insert_DMXzone_Templates" />
		</menu-insert>
		<menu-insert appendTo="DWMenu_Insert_DMXzone_Templates">
			<menuitem name="Responsive Bootstrap Travel Template" codeOnly="FALSE" id="DMXzone_Insert_DMXTempl_ResponsiveBootstrapTravelTemplate" command="dw.runCommand('dmxTemplateManager','installTemplate','ResponsiveBootstrapTravelTemplate','Responsive Bootstrap Travel Template')" />
		</menu-insert>
  </configuration-changes>
  
</macromedia-extension>